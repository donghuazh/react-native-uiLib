{
  "name": "uiLib",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:device": "cd android && ./gradlew assembleRelease && cd .. && react-native run-android --variant=release",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator \"iPhone X\"",
    "ios:5s": "react-native run-ios --simulator \"iPhone 5s\"",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test:jest": "NODE_ENV=test ./node_modules/.bin/jest --config .jestrc --verbose",
    "test": "echo \"No tests found\""
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "mobx": "^5.6.0",
    "mobx-react": "^5.4.3",
    "react": "^16.6.3",
    "react-native": "^0.57.7",
    "react-native-blur": "^3.2.2",
    "react-native-gesture-handler": "^1.0.14",
    "react-native-keyboard-tracking-view": "^5.5.0",
    "react-native-ui-lib": "^3.15.5",
    "react-navigation": "^3.0.9"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.120",
    "@types/react": "^16.7.20",
    "@types/react-native": "^0.57.32",
    "@types/react-navigation": "^3.0.1",
    "@types/react-test-renderer": "^16.0.3",
    "babel-jest": "23.6.0",
    "babel-preset-mobx": "^2.0.0",
    "jest": "23.6.0",
    "jsc-android": "^236355.1.1",
    "metro-react-native-babel-preset": "^0.51.1",
    "prettier": "^1.16.0",
    "react-addons-test-utils": "^15.6.2",
    "react-native-typescript-transformer": "^1.2.11",
    "react-test-renderer": "^16.6.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
